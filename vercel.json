{
  "functions": {
    "api/dexscreener-token.js": { "maxDuration": 30 },
    "api/helium-hotspots.js": { "maxDuration": 60 },
    "api/validators-app.js": { "maxDuration": 60 },
    "api/solana-rpc-proxy.js": { "maxDuration": 60 },
    "api/etf-flows.js": { "maxDuration": 30 }
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/favico/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=604800" }
      ]
    },
    {
      "source": "/offline.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/manifest.webmanifest",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" }
      ]
    }
  ]
}
